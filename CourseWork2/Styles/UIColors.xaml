<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PrimaryBackColor1"> #0B2959 </Color>
    <Color x:Key="PrimaryBackColor2"> #0B3559 </Color>
    <Color x:Key="SecondaryBackColor1"> #124D91 </Color>
    <Color x:Key="SecondaryBackColor2"> #0D3A6D </Color>

    <Color x:Key="BorderColor1"> #377AE6 </Color>
